{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/manifest.json",
      "dest": "/static/manifest.json"
    },
    {
      "src": "/sw.js",
      "dest": "/static/sw.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.py"
    }
  ]
}
